{
  "manifest_version": 2,
  "name": "Look on YouTube",
  "version": "1.0",
  "description": "Open a tab on YouTube with the term you want to look for.",
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "background": {
    "scripts": ["js/main.js"]
  },
  "browser_action": {
    "default_icon": "images/icon.png"
  },
  "icons": {
    "16": "images/icon.png",
    "32": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  }
}